{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": {"version": 3, "proposals": true},
        "modules": "commonjs",
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
  ]
}